<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.DragonAgeRpg"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="17"
              android:targetSdkVersion="17"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name="HelloWorld"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.DragonAgeRpg.DisplayMessageActivity"
                  android:label="@string/display_message_activity_title"
                  android:parentActivityName=".HelloWorld"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HelloWorld"
                />
        </activity>
    </application>
</manifest>
